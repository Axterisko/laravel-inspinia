{
    "name": "axterisko/laravel-inspinia",
    "description": "Laravel Package for integrating Inspinia template and this package is Laravel Mix friendly.",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Pettiti",
            "email": "alex.pettiti@axterisko.it",
            "homepage": "www.axterisko.it"
        }
    ],
    "homepage": "https://github.com/axterisko/laravel-inspinia",
    "keywords": ["Laravel", "Inspinia"],
    "type": "package",
    "require": {
        "illuminate/support": "~5|~6|~7|~8",
        "laravel/ui": "~2|~3",
        "rsmalc/laravel-noty": "^2.3",
        "mariuzzo/laravel-js-localization": "^1.7",
        "yajra/laravel-datatables": "^1.5",
        "spatie/laravel-menu": "^3.6",
        "spatie/laravel-permission": "^3.17",
        "diglactic/laravel-breadcrumbs": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0",
        "laracasts/generators": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Axterisko\\Inspinia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Axterisko\\Inspinia\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Axterisko\\Inspinia\\InspiniaServiceProvider"
            ],
            "aliases": {
                "Inspinia": "Axterisko\\Inspinia\\Facades\\Inspinia"
            }
        }
    }
}
